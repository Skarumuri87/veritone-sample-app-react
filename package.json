    "react": "^15.6.0",
    "react-dom": "^15.6.0",
    "react-redux": "^5.0.5",
    "react-redux-saga": "^1.0.2",
    "react-router": "^4.1.1",
    "react-router-redux": "next",
    "redux": "^3.6.0",
    "redux-api-middleware-fixed": "^1.2.0",
    "redux-batch-enhancer": "^0.1.3",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "cheerio": "^1.0.0-rc.1",
    "functional-permissions-lib": "git+https://github.com/veritone/functional-permissions-lib#develop",
    "node-config": "git+https://github.com/inspirent/node-config",
    "node-logger": "git+https://github.com/inspirent/node-logger",
    "node-sass-chokidar": "^0.0.2",
    "npm-run-all": "^4.0.2",
    "prettier": "^1.4.4",
