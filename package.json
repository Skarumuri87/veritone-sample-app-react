{
  "name": "veritone-sample-app-react",
  "version": "0.1.0",
  "private": true,
  "proxy": {
    "/auth": {
      "target": "http://local.veritone.com:9000"
    }
  },
  "dependencies": {
    "babel-runtime": "^6.23.0",
    "classnames": "^2.2.5",
    "compression": "^1.7.0",
    "cookie-parser": "^1.4.3",
    "date-fns": "^1.28.5",
    "dotenv": "^4.0.0",
    "express": "^4.15.4",
    "lodash": "^4.17.4",
    "material-ui": "^0.18.7",
    "normalize.css": "^7.0.0",
    "passport": "^0.4.0",
    "passport-veritone": "git+https://github.com/veritone/passport-veritone.git",
    "prop-types": "^15.5.10",
    "qs": "^6.5.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.6",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "unirest": "^0.5.1",
    "universal-cookie": "^2.0.8"
  },
  "devDependencies": {
    "cheerio": "^1.0.0-rc.1",
    "functional-permissions-lib": "git+https://github.com/veritone/functional-permissions-lib#develop",
    "node-sass-chokidar": "^0.0.2",
    "npm-run-all": "^4.0.2",
    "prettier": "^1.4.4",
    "react-scripts": "1.0.7",
    "redux-immutable-state-invariant": "^2.0.0",
    "redux-logger": "^3.0.6",
    "redux-validate-fsa": "^0.1.2",
    "veritone-api": "git+https://github.com/veritone/veritone-api#refactor/VTN-3590/use-handler-factory",
    "veritone-lib": "git+https://github.com/veritone/veritone-lib#develop"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "serve": "node server.js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
